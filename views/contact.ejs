<body>
    <div class="main">
        <%- include ('Navbar.ejs') %>
            <div class="ContContent">
                <div class="contimg">
                    <img src="/css/contactimg.jpg" alt="">
                </div>
                <div class="contactCont">
                    <div class="contForm">
                        <h2>Contact us</h2>
                        <form action="/" method="post">

                            <input type="text" name="Name" id="" placeholder="Enter Your Name" autocomplete="off">
                            <input type="tel" name="PhnNum" id="" placeholder="Enter Your Phone Number" id="phone"
                                autocomplete="off">
                            <input type="email" name="Email" id="" placeholder="Enter Your Email" autocomplete="off">
                            <button type="submit" name="Btn">Submit</button>
                        </form>
                        <div class="whtsapp">
                            <h4>WhatsApp Now!!!!</h4>
